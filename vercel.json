
{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/index.js",
      "use": "@vercel/node"
    },
    { "src": "public/*.html", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/dist/src/index.js"
    },
    {
      "src": "/js/.*",
      "dest": "/public/*js"
    }      
  ]
}